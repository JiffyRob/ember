{"ember_version": "0.0.1",
  "asset_directory": "",
  "image_edge": [5,5,5,5],
  
 "elements": {
 
   "text": {"font": ["PixelFont", "pixel"]},
 
   "view": {"transition": ["Fade", 0.15]},

   "container": {
   },

   "button": {
     "default_state":["State",{"element_offset": [0,-1]}],
     "click_state":["State",{"element_offset": [0,0]}],
     "default_size": [100,24]
   },

   "button2":{
     "type": "button"
   },

   "transparent_button": {
     "type": "button"
   },

   "toggle": {
     "default_size": [30, 15],
     "handle_width_ratio": 0.9
   },
   
   "slider": {
     "default_size": [70, 15],
     "handle_width_ratio": 0.9
   },

   "text_field": {
     "prompt_color": [220, 180, 160],
     "highlight_color": [100, 170, 85],
     "default_size": [100,24],
     "default_scroll_size": [["FILL", 1, -8], ["FILL", 1, -10]]
   },

   "scroll": {
     "default_handle_material": ["AverageColor",[0,0,30]],
     "hover_handle_material": ["AverageColor",[0,0,40]],
     "click_handle_material": ["AverageColor",[0,-20,70]],
     "handle_material_transition": ["SurfaceFade", 0.15],
     "padding": 2,
     "scrollbar_size": 2
   }
 },

  "constants": {
    "background_color": [69, 101, 69]
  }
}