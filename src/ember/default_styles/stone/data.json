{"pxui_version": "0.1",
  "asset_directory": "",
  "image_edge": [5,5,5,5],
  
 "elements": {
 
   "text": {"font": ["PixelFont", "pixel"]},
 
   "view": {"transition": ["Fade", 0.15]},

   "stack": {
     "material_transition": ["SurfaceFade", 0.15]
   },

   "button": {
     "element_offset": [0,-1],
     "element_clicked_offset": [0, 0],
     "element_hover_offset": [0,-1],
     "element_highlight_offset": [0,-1],
     "default_size": [50,22]
   },

   "button2":{
     "type": "button"
   },

   "transparent_button": {
     "type": "button",
     "element_hover_offset": [0, -1],
     "element_clicked_offset": [0, -1],
     "element_highlight_offset": [0,-1]
   },

   "toggle": {},
   
   "slider": {},

   "text_field": {
     "prompt_color": [220, 180, 160],
     "highlight_color": [100, 170, 85],
     "default_size": [50,20],
     "default_scroll_size": [["FILL", 1, -8], ["FILL", 1, -10]]
   },

   "list": {
     "highlight_material": ["AverageColor",[0,0,10]],
     "highlight_focus_material": ["AverageColor",[0,0,20]],
     "material_transition": ["SurfaceFade", 0.15],

     "text_width": 40,
     "text_height": 18
   },

   "scroll": {
     "handle_material": ["AverageColor",[0,0,30]],
     "handle_hover_material": ["AverageColor",[0,0,40]],
     "handle_click_material": ["AverageColor",[0,-20,70]],
     "handle_material_transition": ["SurfaceFade", 0.15],
     "padding": 3
   }
 },

  "constants": {
    "background_color": [69, 101, 69],
    "background_color_dark": [59, 81, 59],
    "background_color_light": [89, 121, 89]
  }
}