{"ember_version": "0.0.1",
  "asset_directory": "",
  "image_edge": [5,5,5,5],
  
 "elements": {
 
   "Text": {"font": ["PixelFont", "pixel"]},

   "View": {},

   "Container": {
     "sizes": {"Layout": "FILL"},
     "min_spacing": 6
   },

    "Section": {
        "focus_material": ["RoundedRect", [255, 255, 255], 3, 1, false]
    },

   "Button": {
     "default_state":["State",{"element_offset": [0,-1]}],
     "click_state":["State",{"element_offset": [0,0]}],
     "size": [100,24]
   },

   "button2":{
     "type": "Button"
   },

   "transparent_button": {
     "type": "Button"
   },

   "Toggle": {
     "size": [30, 15],
     "handle_width_ratio": 0.9
   },
   
   "Slider": {
     "size": [70, 15],
     "handle_width_ratio": 0.9
   },

   "text_field_scroll": {
     "type": "Scroll",
     "default_handle_material": ["AverageColor",[0,0,30]],
     "hover_handle_material": ["AverageColor",[0,0,40]],
     "click_handle_material": ["AverageColor",[0,-20,70]],
     "handle_material_transition": ["SurfaceFade", 0.15],
     "padding": 2,
     "scrollbar_size": 2
   },

   "TextField": {
     "highlight_color": [100, 170, 85],
     "size": [100,24],
     "scroll_style": ["Style", "text_field_scroll"],
     "default_scroll_size": [["FILL", 1, -8], ["FILL", 1, -10]]
   },

   "Scroll": {
     "default_handle_material": ["AverageColor",[0,0,30]],
     "hover_handle_material": ["AverageColor",[0,0,40]],
     "click_handle_material": ["AverageColor",[0,-20,70]],
     "handle_material_transition": ["SurfaceFade", 0.15],
     "padding": 6,
     "scrollbar_size": 3
   }
 },

  "constants": {
    "background_color": [69, 101, 69]
  }
}